/* Hide the browser's default checkbox */
.checkbox_css input {

  position: absolute;
  opacity: 0;
  cursor: pointer;
  left: 0px;
  width: 15px;
  z-index: 999;
  top: 3px;
  height: 15px !important;
}

/* Create a custom checkbox */
.checkmark {
  border:1px solid #333;
  position: absolute;
  top: 0;
  left: 0;
  height: 15px;
  width: 15px;
}
