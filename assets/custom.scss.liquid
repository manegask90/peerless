      -o-transition: all .2s ease-in-out;
      transition: all .2s ease-in-out;
      &:before {
        content: "\f0dd";
        position: absolute;
        bottom: 0;
        left: 50%;
        right: auto;
        color: #000;
        font-size: 16px;
        font-family: FontAwesome;
        line-height: 5px;
        -webkit-transform: translateX(-50%) translateY(0);
