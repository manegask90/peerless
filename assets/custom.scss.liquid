.header-full-width {
  .topbar-wrapp > .container,
  .main-header > .container,i
  .sticky-header > .container {
    width: calc(100% - 70px);
    @media screen and (max-width: 767px) {
      width: calc(100% - 20px);
    }
    @media screen and (max-width: 480px) {
      padding: 0;
    }
  }
}
.template-page .main-header,
.template-page .header-wrap {
  margin-bottom: 0;
}

.container {
  padding: 0 15px;
}

.container .row,
.container-fluid .row {
  width: unset;
}

/* Search Bar */
body .basel-search-full-screen {
  .basel-search-wrapper.search-overlap {
    transform: translate(0, 7px);
  }
  .basel-search-inner {
    padding: 20px;
    .search-option {
      color: #ffffff;
      font-family: "Helvetica Neue LT Std";
      font-size: 14px;
      letter-spacing: 0.3px;
      line-height: 21px;
      text-align: center;
      text-align-last: center;
      &::-ms-expand {
        color: #fff;
        display: none;
      }
    }
    input[type="text"] {
      color: #191919;
      font-family: "Helvetica Neue", sans-serif;
      font-size: 14px;
      letter-spacing: 0.3px;
      line-height: 21px;
      font-style: normal;
      padding: 10px 20px;
      border-width: 1px 0px;
      &::placeholder {
        color: #9b9b9b;
      }
    }
    button {
      padding: 0 11px;
    }
  }
}

/* Breadcrumb */
.single-breadcrumbs-wrapper {
  padding: 0;
  margin-bottom: 13px;
  .shopify-breadcrumb {
    padding: 0;
    a {
      color: #000;
      font-family: "Helvetica Neue LT Std";
      font-size: 12px;
      line-height: 14px;
    }
    .breadcrumb-last {
      color: #9b9b9b;
      font-family: "Helvetica Neue LT Std";
      font-size: 12px;
      line-height: 14px;
    }
  }
}

/* Header burger */
.mobile-nav-icon .basel-burger,
.mobile-nav-icon .basel-burger:before,
.mobile-nav-icon .basel-burger:after {
  @media screen and (max-width: 767px) {
    // background: #fff;
  }
}
.act-scroll .mobile-nav-icon .basel-burger,
.act-scroll .mobile-nav-icon .basel-burger:before,
.act-scroll .mobile-nav-icon .basel-burger:after {
  @media screen and (max-width: 767px) {
    background: rgba(0, 0, 0, 0.9);
  }
}

.sticky-header .site-logo {
  @media screen and (max-width: 991px) {
    width: calc(100% - 200px);
  }
}

/* Footer */
.mc4wp-form input[type="email"] {
  color: #000;
  font-family: "Helvetica Neue LT Std";
  font-size: 13px;
  letter-spacing: 0.5px;
  line-height: 22px;
  &::placeholder {
    color: #9b9b9b;
    font-family: "Helvetica Neue LT Std";
    font-size: 13px;
    letter-spacing: 0.5px;
    line-height: 22px;
