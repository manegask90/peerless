/* Compare Popup */
#cboxContent {
  #cboxLoadedContent {
    margin: 0;
  }
  #cboxClose {
    color: #ffffff;
    font-family: "Helvetica Neue LT Std";
    font-size: 27px;
    font-weight: 500;
    line-height: 28px;
    bottom: -32px;
    padding-right: 30px;
    &:before,
    &:after {
      width: 22px;
      height: 2px;
      top: 13px;
      transform: rotate(45deg);
    }
    &:after {
      transform: rotate(-45deg);
    }
  }
}
#compare-content {
  position: relative;
  &::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, 0.9);
    z-index: 2;
    display: none;
  }
  &::after {
    content: '';
    position: absolute;
    top: calc(50vh - 90px);
    left: 50%;
    margin-right: -10px;
    z-index: 2;
    width: 20px;
    height: 20px;
    display: none;
    border: 2px solid #000;
    border-top-color: transparent;
    border-radius: 100%;
    animation: load-spin 450ms infinite linear;
    -o-animation: load-spin 450ms infinite linear;
    -ms-animation: load-spin 450ms infinite linear;
    -webkit-animation: load-spin 450ms infinite linear;
    -moz-animation: load-spin 450ms infinite linear;
  }
  &.loading {
    &:before,
    &:after {
      display: block;
    }
  }
}
