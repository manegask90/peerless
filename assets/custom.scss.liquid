/** Timeline section. Page: About us */
// timeline sequence
body .main-page-wrapper.about-page .about-timeline-wrapper {
  padding: 75px 0 380px;
  position: relative;
  @media (max-width: 1199px) {
    padding-bottom: 0;
  }
  @media (max-width: 767px) {
    padding-top: 38px;
  }
}

div.timeline-sequence {
  border-bottom: 3px solid #ee3423;
  .timeline-inner-wrap {
    display: flex;
  }
  .sequence-item {
    float: left;
    position: relative;
    padding: 0 10px;
    min-width: 30px;
    min-height: 44px;
    // &:not(.sequence-main-item) {
    cursor: pointer;
    // }
    &:last-child {
      margin-left: 20px !important;
    }
    &:nth-last-child(2) {
      margin-left: 20px !important;
    }
    &.sequence-main-item {
      min-width: 55px;
      &.sequence-item-22 {
        margin-left: 87px !important;
      }
      &.sequence-item-23 {
        margin-left: 87px !important;
      }
      .timeline-slider-item {
        position: absolute;
        top: 100%;
        left: -50%;
        margin-left: 25px;
        margin-top: 20px;
        min-width: 134px;
        transition: all 0.2s ease-in-out;
        .timeline-slider-title {
          color: #ee3423;
          font-size: 22px;
          font-weight: 500;
        }
        .timeline-slider-content {
          color: #9b9b9b;
        }
        .timeline-slider-img {
          img {
            max-width: 110px;
          }
        }
      }
      &.sequence-main-item-active {
        &:after {
          width: 22px;
          height: 22px;
          background: radial-gradient(circle closest-side, #fff 6px, #191919 7px);
          bottom: -12.5px;
          margin-left: -11px;
          border: 1px solid #ee3423;
        }
        .timeline-slider-item {
          margin-top: 32px;
          transform: scale(1.1);
          .timeline-slider-content {
            color: #fff;
          }
        }
      }
    }
    &:after {
      content: "";
      display: block;
      width: 14px;
      height: 14px;
      background-color: #ee3423;
      border-radius: 50%;
      position: absolute;
      bottom: -8.5px;
      left: 50%;
      margin-left: -7px;
    }
    &.sequence-main-item:after {
      width: 20px;
      height: 20px;
      background-color: #191919;
      border: 3px solid #ee3423;
      border-radius: 50%;
      bottom: -11.5px;
      left: 50%;
      margin-left: -10px;
      transition: all 0.4s ease-in-out;
    }

    .sequence-title {
      color: #9b9b9b;
      text-align: center;
      font-size: 16px;
      font-weight: 500;
    }

    .sequence-tooltip {
      visibility: hidden;
      opacity: 0;
      min-width: 134px;
      position: absolute;
      bottom: 100%;
      left: 50%;
      z-index: 100;
      padding: 10px;
      margin-bottom: 15px;
      background-color: #ee3423;
      color: #fff;
      font-size: 12px;
      line-height: 18px;
      cursor: initial;
      -webkit-transition: opacity 0.5s ease-in;
      -moz-transition: opacity 0.5s ease-in;
      -ms-transition: opacity 0.5s ease-in;
      -o-transition: opacity 0.5s ease-in;
      transition: opacity 0.5s ease-in;
      &:after {
        content: "";
        display: block;
        width: 0px;
        height: 0px;
        border-left: 0px solid transparent;
        border-right: 20px solid transparent;
        border-top: 15px solid #ee3423;
        position: absolute;
        left: 0;
        bottom: -13px;
      }
    }
    &:hover {
      .sequence-tooltip {
        visibility: visible;
        opacity: 1;
      }
    }
  }

  @media (max-width: 1199px) {
    & {
      border-bottom: 0;
      position: relative;
      &:before {
        content: "";
        position: absolute;
        top: 144px;
        border-bottom: 3px solid #ee3423;
        width: 100%;
        left: 0;
      }
    }
    .container {
      > div {
        overflow: hidden;
        padding-bottom: 380px;
        padding-top: 100px;
      }
    }
    .timeline-inner-wrap {
      width: max-content;
      width: intrinsic;
      width: -moz-max-content;
      width: -webkit-max-content;
      transition: 0.5s;
      .sequence-item:first-child {
        //min-width: 0;
        //width: 0;
      }
    }
    .sequence-item {
      &.sequence-main-item {
        &.sequence-item-22 {
          margin-left: 0 !important;
        }
        &.sequence-item-23 {
          margin-left: 0 !important;
        }
        .timeline-slider-item {
          left: 0;
          padding-right: 80px;
          margin-left: 0;
        }
        &.sequence-main-item-active {
          .timeline-slider-item {
            transform: scale(1);
          }
        }
      }
    }
  }
}

.timeline-slider-wrapper {
  position: relative;
  @media (max-width: 1199px) {
    margin-top: 0 !important;
  }
}

.timeline-slide-btn {
  display: none;
