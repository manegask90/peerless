                {%- if order.cancelled -%}
                  {%- assign cancelled_at = order.cancelled_at | date: "%B %d, %Y %I:%M%p" -%}
                  <p>{% comment %}ly_i18n_replace_for_[ 'customer.order.cancelled' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.cancelled' | t }}{% endcapture %}{% include 'ly-static-string' with '13491677' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <mark class="order-cancelled">{{cancelled_at}}</mark></p>
                  <p>{% comment %}ly_i18n_replace_for_[ 'customer.order.cancelled_reason' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.cancelled_reason' | t }}{% endcapture %}{% include 'ly-static-string' with '13491678' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <mark class="order-cancelled_reason">{{order.cancel_reason}}</mark></p>
