                            <td class="shopify-table__product-name product-name">
                              <a href="{{ line_item.url | within: collections.all }}">{% comment %}ly_code_replace_for_[line_item.product.title]_begin{% endcomment %}{% include 'ly-title' with line_item.product %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}{%- if line_item.product.has_only_default_variant == false %} – {% for option in line_item.product.options -%}{{ option | capitalize }}: {{ line_item.variant.options[forloop.index0] }}{%- if forloop.last == false-%}, {% endif -%}{%- endfor -%}{%- endif -%}</a> <strong class="product-quantity">× {{ line_item.quantity }}</strong>
