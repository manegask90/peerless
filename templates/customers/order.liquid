                    <div>
                      {% if order.metafields.IFS_data.IFSOrderId %}
                        <span class="custom_metafields"><label><ly-as-13491666>Peerless Order ID:</ly-as-13491666></label> {% comment %}ly_code_replace_for_[order.metafields.IFS_data.IFSOrderId]_begin{% endcomment %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% assign ns = language | append: 'IFS_data' %}{% assign key = 'IFSOrderId' %}{% if order.metafields[ns][key] %}{% assign ly_translation = order.metafields[ns][key] %}{% else %}{% assign ly_translation = order.metafields.IFS_data.IFSOrderId %}{% endif %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}</span>
                      {% endif %}

                      {% if order.metafields.IFS_data.IFSOrderId %}
                        <span class="custom_metafields custom_metafields_right_span"><label><ly-as-13491667>Expected Ship To Date:</ly-as-13491667></label> {% comment %}ly_code_replace_for_[order.metafields.IFS_data.EXPECTEDSHIPTODATE]_begin{% endcomment %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% assign ns = language | append: 'IFS_data' %}{% assign key = 'EXPECTEDSHIPTODATE' %}{% if order.metafields[ns][key] %}{% assign ly_translation = order.metafields[ns][key] %}{% else %}{% assign ly_translation = order.metafields.IFS_data.EXPECTEDSHIPTODATE %}{% endif %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}</span>
                      {% endif %}
                    </div>
                    <table class="shopify-table shopify-table--order-details shop_table order_details">
                      <thead>
                        <tr>
                          <th class="shopify-table__product-name product-name">{% comment %}ly_i18n_replace_for_[ 'customer.order.product' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.product' | t }}{% endcapture %}{% include 'ly-static-string' with '13491680' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
                          <th class="shopify-table__product-table product-total">{% comment %}ly_i18n_replace_for_[ 'customer.order.total' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.total' | t }}{% endcapture %}{% include 'ly-static-string' with '13491681' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</th>
