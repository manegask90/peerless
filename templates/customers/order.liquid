                          {%- if order.note != blank -%}
                            <tr>
                              <th>{% comment %}ly_i18n_replace_for_[ 'customer.order.note' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.note' | t }}{% endcapture %}{% include 'ly-static-string' with '13491690' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}:</th>
                              <td>{{ order.note }}</td>
                            </tr>
                          {%- endif -%}
                        </tbody>
                      </table>
                    </div>
                    <section class="shopify-columns shopify-columns--2 shopify-columns--addresses col2-set addresses">
                      <div class="shopify-column shopify-column--1 shopify-column--billing-address col-1">
                        <h3 class="shopify-column__title">{% comment %}ly_i18n_replace_for_[ 'customer.order.billing_address' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.billing_address' | t }}{% endcapture %}{% include 'ly-static-string' with '13491691' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h3>
                        <p><strong>{% comment %}ly_i18n_replace_for_[ 'customer.order.payment_status' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'customer.order.payment_status' | t }}{% endcapture %}{% include 'ly-static-string' with '13491692' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}:</strong> {{ order.financial_status_label }}</p>
                        {%- assign order_billing =  order.billing_address | format_address -%}
                        {{order_billing | replace: '<p>', '<address><ly-as-13491668>' | replace: '</ly-as-13491668></p>', '</address>' }}
                      </div>
                      <!-- /.col-1 -->
