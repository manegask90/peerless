<script type="text/javascript">
  $( document ).ready(function() {

    $(".address-edit-toggle").click(function() {
      var address_id = $(this).data('form-id');
      if (!$("#EditAddress_" + address_id).hasClass('hide')) {
        $(this).parent().hide();
      }
    });

    $(".address-edit-toggle").click(function() {
      var address_id = $(this).data('form-id');
      if (!$("#EditAddress_" + address_id).hasClass('hide') && !$(this).hasClass('btn_cancel')) {
        $(this).parent().hide();
      }
      if($(this).hasClass('btn_cancel')) {
        $("#EditAddress_" + address_id).prev().find("p:last-child").show();
      }
    });

    $('.btn_cancel').click(function() {
      $('.basel-my-account-sidebar').css('position', 'initial');
    })

    $(".address-new-toggle").on('click', function(){
      var address_count = $(this).data("address-count");
      if(address_count >= 5) {
        $("#AddressNewForm").hide();
        $(".account-wrapper").css("z-index", 1000);
        $("body").css("overflow", "hidden");
        $("#add-popup-wrapper").show();
      } else {
          $(".main-page-wrapper").toggleClass('top-pos');
          $("body").toggleClass('fixed-pos');
        }
      });

    $('.address-popup-delete').on('click', function() {
      var $el = $(this);
      var formId = $el.data('form-id');
      var confirmMessage = $el.data('confirm-message');
      $(".account-wrapper").css("z-index", 1000);
      $("body").css("overflow", "hidden");
      $("#delete-popup-wrapper").show();
      $("#delete-popup-wrapper").attr('data-form-id', formId);
    });

    $("body").on("click", "#delete-popup-wrapper .btn", function(){
      if($(this).hasClass('btn-confirm')) {
        var formId = $("#delete-popup-wrapper").attr('data-form-id');
        Shopify.postLink('/account/addresses/' + formId, {parameters: {_method: 'delete'}});
      } else {
        $(".account-wrapper").css("z-index", 1);
        $("body").css("overflow", "auto");
        $("#delete-popup-wrapper").hide();
      }
    });

    $("body").on("click", "#add-popup-wrapper .btn", function(){
      $(".account-wrapper").css("z-index", 1);
      $("body").css("overflow", "auto");
      $("#add-popup-wrapper").hide();
    });

    $('#address_form_new input[type="submit"]').on('click', function () {
      var form = $(this).closest('form');
      if (form[0].checkValidity()) { // valid
        form.removeClass('invalid');
      } else {
        form.addClass('invalid');
      }
    });

    var sel_country = document.body.querySelectorAll("select.address-country-option-custom");

    for (var i = 0; i < sel_country.length; i++) {

      sel_country[i].onchange = function() {
        if(this.value == 'United States') {
          var parent = this.parentNode.parentNode;
          var select_usa = parent.querySelector("select[name='address[province]']");
          
          select_usa.classList.add('usa-reg');

        }
      }

      //if(sel_country[i].value == 'United States') {
        var parent = sel_country[i].parentNode.parentNode;
        var select_usa = parent.querySelector("select[name='address[province]']");

        select_usa.classList.add('usa-reg');
      //}
    }

    $(document).on('change', 'select.usa-reg', function() {
      var select_reg = $(this).val();
      if(select_reg == 'Alaska' || select_reg == 'Hawaii' || select_reg == 'Puerto Rico'){
        $('#usa-reg-popup').show();
      }
    });

    $("body").on("click", "#usa-reg-popup .btn", function(){
      $("#usa-reg-popup").hide();
    });
  });

  var addressCount = $('.addresses .address').length;
  if (addressCount < 1) {
  }
</script>

