{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<div class="main-page-wrapper">
	<!-- MAIN CONTENT AREA -->
	<div class="container">
		<div class="row">
			<div class="site-content col-sm-12" role="main">
				<article id="post-{{page.id}}" class="post-{{page.id}} page type-page status-publish hentry">
					<div class="entry-content">
						{%- comment -%}<ly-as-13491814>faq,question,tab,counter,page_content</ly-as-13491814>{%- endcomment -%}
						{%- if settings.sort_faq != blank -%}
							{%- assign _sort_faq = settings.sort_faq | replace: ' >', '>' | replace: '> ', '>' | split: '>' | uniq -%}
							{%- for el_faq in _sort_faq -%}
								{%- case forloop.index -%} 
									{%- when 1 -%}
										{%- if el_faq  == 'faq' -%}
											{%- section 'faqs_faq' -%}
										{%- elsif el_faq  == 'question' -%}
											{%- section 'faqs_question' -%} 
										{%- elsif el_faq  == 'tab' -%}
											{%- section 'faqs_tab' -%} 
										{%- elsif el_faq  == 'counter' -%}
											{%- section 'faqs_counter' -%}
										{%- elsif el_faq  == 'page_content' -%}
											{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
										{%- endif -%}
									{%- when 2 -%}
										{%- if el_faq  == 'faq' -%}
											{%- section 'faqs_faq' -%}
										{%- elsif el_faq  == 'question' -%}
											{%- section 'faqs_question' -%} 
										{%- elsif el_faq  == 'tab' -%}
											{%- section 'faqs_tab' -%} 
										{%- elsif el_faq  == 'counter' -%}
											{%- section 'faqs_counter' -%}
										{%- elsif el_faq  == 'page_content' -%}
											{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
										{%- endif -%}
									{%- when 3 -%}
										{%- if el_faq  == 'faq' -%}
											{%- section 'faqs_faq' -%}
										{%- elsif el_faq  == 'question' -%}
											{%- section 'faqs_question' -%} 
										{%- elsif el_faq  == 'tab' -%}
											{%- section 'faqs_tab' -%} 
										{%- elsif el_faq  == 'counter' -%}
											{%- section 'faqs_counter' -%}
										{%- elsif el_faq  == 'page_content' -%}
											{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
										{%- endif -%}
									{%- when 4 -%}
										{%- if el_faq  == 'faq' -%}
											{%- section 'faqs_faq' -%}
										{%- elsif el_faq  == 'question' -%}
											{%- section 'faqs_question' -%} 
										{%- elsif el_faq  == 'tab' -%}
											{%- section 'faqs_tab' -%} 
										{%- elsif el_faq  == 'counter' -%}
											{%- section 'faqs_counter' -%}
										{%- elsif el_faq  == 'page_content' -%}
											{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
										{%- endif -%}
									{%- when 5 -%}
										{%- if el_faq  == 'faq' -%}
											{%- section 'faqs_faq' -%}
										{%- elsif el_faq  == 'question' -%}
											{%- section 'faqs_question' -%} 
										{%- elsif el_faq  == 'tab' -%}
											{%- section 'faqs_tab' -%} 
										{%- elsif el_faq  == 'counter' -%}
											{%- section 'faqs_counter' -%}
										{%- elsif el_faq  == 'page_content' -%}
											{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
										{%- endif -%}
								{%- endcase -%}
									{% comment %}{%- section 'el_faq' -%}{% endcomment %}
							{%- endfor -%}
						{%- else -%}
							{%- comment -%}<ly-as-13491815>faq info</ly-as-13491815>{%- endcomment -%}
							{%- section 'faqs_faq' -%}
							{%- comment -%}<ly-as-13491816>Question General Information</ly-as-13491816>{%- endcomment -%}
							{%- section 'faqs_question' -%} 
							{%- comment -%}<ly-as-13491817>faq tab</ly-as-13491817>{%- endcomment -%}
							{%- section 'faqs_tab' -%} 
							{%- comment -%}<ly-as-13491818>basel-counter</ly-as-13491818>{%- endcomment -%}
							{%- section 'faqs_counter' -%}
							{% comment %}ly_code_replace_for_[page.content]_begin{% endcomment %}{% include 'ly-content' with page %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}
						{%- endif -%}
					</div>
				</article>
			</div><!-- .site-content -->
		</div><!-- end row -->
	</div><!-- end container -->
</div><!-- .main-page-wrapper -->
