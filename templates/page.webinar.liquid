{% raw %}
   <script id="webinar-template" type="text/x-handlebars-template">
      <div class="article-item" data-id="{{sessionKey}}">
         <div class="article-image">
            <img src="{{image}}" alt="{{sessionName}}">
         </div>
         <div class="article-wrapper">
            <h2 class="article-title">{{sessionName}}</h2>
            <div class="webinar-plan">
               <span class="date-wrapper">{{date}}</span>
               <span class="split-data-time"> | </span>
               <span class="time-wrapper">{{time}}</span>
            </div>
            <div class="tags-wrapper">
               <span class="tag-item spots_available">Spots available</span>
            </div>
            <p class="article-content">{{{description}}}</p>
         </div>
         <div class="article-actions">
            <a href="javascript:void(0)"><span>View Details</span></a>
         </div>
         <div class="article-more"></div>
      </div>
   </script>
{% endraw %}

{{ 'https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.1.0/handlebars.js' | script_tag }}

{% comment %}ly_asset_replace_for_[ 'webinars.js' | asset_url | script_tag ]_begin{% endcomment %}{% assign ly_asset = 'webinars.js' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | script_tag }}{% comment %}ly_asset_replace_end{% endcomment %}