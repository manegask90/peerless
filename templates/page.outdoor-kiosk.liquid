{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}
<div class="main-page-wrapper kiosk-outdoor">	
	{% section 'kiosk-outdoor' %}
	<div class="container">
		<div class="row">
			<div class="shop-loop-head">
               <nav class="shopify-breadcrumb"><a href="/">{% comment %}ly_i18n_replace_for_[ 'general.breadcrumbs.home' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.breadcrumbs.home' | t }}{% endcapture %}{% include 'ly-static-string' with '13492006' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a><span class="breadcrumb-last"> {% comment %}ly_code_replace_for_[page.title | capitalize]_begin{% endcomment %}{% include 'ly-title' with page %}{{ly_translation | capitalize}}{% comment %}ly_code_replace_end{% endcomment %}</span></nav>
               {%- if settings.shop_show_per_page %}
              <form class="shopify-ordering shopify-show-per-page" method="get" action="{{cat_link_nathan_xx}}">
                  {%- assign show = settings.products_per_page -%}
                  <label for="show" class="label_show">{% comment %}ly_i18n_replace_for_[ 'collections.sorting.show' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.show' | t }}{% endcapture %}{% include 'ly-static-string' with '13492007' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
                  <select name="show" id="show" class="orderby collection-show__input orderbyshow">
                      <option data-value="{{cat_link_nathan_xx }}?&view=list" value="manual"{%- if check_template_list == false %} selected="selected"{%- endif -%}>12</option>
                      <option data-value="{{cat_link_nathan_xx }}?&view=list_24" value="best-selling"{%- if template.suffix == "list_24" %} selected="selected"{%- endif -%}>24</option>
                      <option data-value="{{cat_link_nathan_xx }}?&view=list_36" value="best-selling"{%- if template.suffix == "list_36" %} selected="selected"{%- endif -%}>36</option>
                      <option data-value="{{cat_link_nathan_xx }}?&view=list_50" value="title-ascending"{%- if template.suffix == "list_50"  %}selected="selected"{%- endif -%}>50</option>
                  </select>
                  <input class="collection-header__default-show" type="hidden" value="{{show}}" />
               </form>
              {%- endif -%}
              {%- if settings.shop_filters %}
               <div class="basel-filter-buttons">
                  <a href="#" class="open-filters">{% comment %}ly_i18n_replace_for_[ 'collections.filters.filters_title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.filters.filters_title' | t }}{% endcapture %}{% include 'ly-static-string' with '13492008' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
               </div>
               {%- else -%}
               <form class="shopify-ordering" method="get" action="{{cat_link_nathan_xx}}">
                  {%- assign sort_by = collection.sort_by | default: collection.default_sort_by -%}
                  <label for="sort_by" class="label--hidden hide hidden">{% comment %}ly_i18n_replace_for_[ 'collections.sorting.title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.title' | t }}{% endcapture %}{% include 'ly-static-string' with '13492009' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
                  <select name="sort_by" id="sort_by" class="collection-sort__input orderby">
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'manual' }}{{sort_symboy}}sort_by=manual" value="manual"{%- if sort_by == "manual" or sort_by == "manual?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.featured' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.featured' | t }}{% endcapture %}{% include 'ly-static-string' with '13492010' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'best-selling' }}{{sort_symboy}}sort_by=best-selling" value="best-selling"{%- if sort_by == "best-selling" or sort_by == "best-selling?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.best_selling' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.best_selling' | t }}{% endcapture %}{% include 'ly-static-string' with '13492011' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'title-ascending' }}{{sort_symboy}}sort_by=title-ascending" value="title-ascending"{%- if sort_by == "title-ascending" or sort_by == "title-ascending?" %}selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.az' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.az' | t }}{% endcapture %}{% include 'ly-static-string' with '13492012' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'title-descending' }}{{sort_symboy}}sort_by=title-descending" value="title-descending"{%- if sort_by == "title-descending" or sort_by == "title-descending?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.za' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.za' | t }}{% endcapture %}{% include 'ly-static-string' with '13492013' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'price-ascending' }}{{sort_symboy}}sort_by=price-ascending" value="price-ascending"{%- if sort_by == "price-ascending" or sort_by == "price-ascending?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.price_ascending' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.price_ascending' | t }}{% endcapture %}{% include 'ly-static-string' with '13492014' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'price-descending' }}{{sort_symboy}}sort_by=price-descending" value="price-descending"{%- if sort_by == "price-descending" or sort_by == "price-descending?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.price_descending' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.price_descending' | t }}{% endcapture %}{% include 'ly-static-string' with '13492015' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'created-descending' }}{{sort_symboy}}sort_by=created-descending" value="created-descending"{%- if sort_by == "created-descending" or sort_by == "created-descending?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.date_descending' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.date_descending' | t }}{% endcapture %}{% include 'ly-static-string' with '13492016' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                      <option data-value="{{cat_link_nathan_xx | replace: sort_by, 'created-ascending' }}{{sort_symboy}}sort_by=created-ascending" value="created-ascending"{%- if sort_by == "created-ascending" or sort_by == "created-ascending?" %} selected="selected"{%- endif -%}>{% comment %}ly_i18n_replace_for_[ 'collections.sorting.date_ascending' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'collections.sorting.date_ascending' | t }}{% endcapture %}{% include 'ly-static-string' with '13492017' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</option>
                  </select>
                  <input class="collection-header__default-sort" type="hidden" value="{{ collection.default_sort_by }}" />
               </form>
               {%- endif -%}
            </div>
			{% section 'gl_section_product_gird_kiosk' %}
		</div>
	</div>
</div>
<script type="text/javascript">
	$(document).ready(function(){
		$('.main-header').addClass('kiosk-header');
        $('.header-wrap').addClass('kiosk-header');
	})
</script>