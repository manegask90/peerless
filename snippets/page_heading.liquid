{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}{%- assign show_breadcrums = false -%}{%- if settings.page_show_breadcrumbs -%}{%- assign show_breadcrums = true -%}{%- endif -%}
{%- assign show_title = true -%}{%- if settings.page_title_design == 'disable' -%}{%- assign show_title = false -%}{%- endif -%}
<div class="gl_page_title page-title page-title-default title-size-{{settings.page_title_size}} color-scheme-{{settings.page_title_color}} title-design-{{settings.page_title_design}}">
	<div class="container">
		<header class="entry-header">
			{%- if template.name != 'article' -%}
				{%- if show_title -%}<h1 class="entry-title">{%- if template.name != 'blog' -%}{{ langify_title }}{%- else -%}{% comment %}ly_code_replace_for_[blog.title]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}{%- endif -%}</h1>{%- endif -%}							
				{%- if show_breadcrums -%}<div class="breadcrumbs" xmlns:v="http://rdf.data-vocabulary.org/#"><a href="/" rel="v:url" property="v:title">{% comment %}ly_i18n_replace_for_[ 'general.breadcrumbs.home' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.breadcrumbs.home' | t }}{% endcapture %}{% include 'ly-static-string' with '13490508' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a> » <span class="current">{{ langify_title }}</span></div><!-- .breadcrumbs -->{%- endif -%}						
			{%- elsif blog_portfolio -%}
				{%- if show_title -%}<h1 class="entry-title">{% comment %}ly_code_replace_for_[blog.title]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}</h1>{%- endif -%}							
				{%- if show_breadcrums -%}<div class="breadcrumbs" xmlns:v="http://rdf.data-vocabulary.org/#"><a href="/" rel="v:url" property="v:title">{% comment %}ly_i18n_replace_for_[ 'general.breadcrumbs.home' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.breadcrumbs.home' | t }}{% endcapture %}{% include 'ly-static-string' with '13490508' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a> » <span class="current">{% comment %}ly_i18n_replace_for_[ 'blogs.portfolio.project' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'blogs.portfolio.project' | t }}{% endcapture %}{% include 'ly-static-string' with '13490509' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</span></div><!-- .breadcrumbs -->{%- endif -%}
			{%- elsif article_portfolio -%}
				{%- if show_title -%}<h1 class="entry-title">{% comment %}ly_code_replace_for_[blog.title]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ly_translation}}{% comment %}ly_code_replace_end{% endcomment %}</h1>{%- endif -%}
				{%- if show_breadcrums -%}<div class="breadcrumbs" xmlns:v="http://rdf.data-vocabulary.org/#"><a href="/" rel="v:url" property="v:title">{% comment %}ly_i18n_replace_for_[ 'general.breadcrumbs.home' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.breadcrumbs.home' | t }}{% endcapture %}{% include 'ly-static-string' with '13490508' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a> » <span typeof="v:Breadcrumb"><a rel="v:url" property="v:title" href="{{blog.url}}">{% comment %}ly_code_replace_for_[blog.title | capitalize ]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ly_translation | capitalize }}{% comment %}ly_code_replace_end{% endcomment %}</a></span> » <span class="current">{% comment %}ly_code_replace_for_[article.title | capitalize]_begin{% endcomment %}{% include 'ly-title' with article %}{{ly_translation | capitalize}}{% comment %}ly_code_replace_end{% endcomment %}</span></div><!-- .breadcrumbs -->{%- endif -%}	
			{%- else -%}
				{%- if show_title -%}<h1 class="entry-title">{% comment %}ly_i18n_replace_for_[ 'blogs.blog.breadcrumbs_article' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'blogs.blog.breadcrumbs_article' | t }}{% endcapture %}{% include 'ly-static-string' with '13490510' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h1>{%- endif -%}	
				{%- if show_breadcrums -%}<div class="breadcrumbs" xmlns:v="http://rdf.data-vocabulary.org/#"><a href="/" rel="v:url" property="v:title">{% comment %}ly_i18n_replace_for_[ 'general.breadcrumbs.home' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.breadcrumbs.home' | t }}{% endcapture %}{% include 'ly-static-string' with '13490508' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a> » <span typeof="v:Breadcrumb"><a rel="v:url" property="v:title" href="{{blog.url}}">{% comment %}ly_code_replace_for_[blog.title | capitalize ]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ly_translation | capitalize }}{% comment %}ly_code_replace_end{% endcomment %}</a></span></div><!-- .breadcrumbs -->{%- endif -%}						
			{%- endif -%}
		</header><!-- .entry-header -->
	</div>
</div>